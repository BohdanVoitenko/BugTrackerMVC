@foreach (var company in Model)
{
    <tr class="text-light tr-hover clickable-row" style="font-size:12px;">
        <td hidden>@company.Id</td>
        <td>
            @company.UserName
        </td>
        <td>
            @company.Email
        </td>
        <td>
            @company.PhoneNumber
        </td>
    </tr>
}